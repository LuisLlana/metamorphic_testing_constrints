@incollection{PAPADAKIS2019275,
title = {Chapter Six - Mutation Testing Advances: An Analysis and Survey},
editor = {Atif M. Memon},
series = {Advances in Computers},
publisher = {Elsevier},
volume = {112},
pages = {275-378},
year = {2019},
