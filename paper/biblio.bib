	keywords = {Computational modeling, Context, Mutation testing, Benchmark testing, Electronic mail, Heuristic algorithms, dynamic subsumption, minimal mutant sets, Minimization},
	pages = {21--30},
	file = {IEEE Xplore Abstract Record:/Users/agd516/Zotero/storage/GLX2ATLH/6823862.html:text/html;IEEE Xplore Full Text PDF:/Users/agd516/Zotero/storage/68MD2QCA/Ammann et al. - 2014 - Establishing Theoretical Minimal Sets of Mutants.pdf:application/pdf},
}

